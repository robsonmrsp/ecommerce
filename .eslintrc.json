{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "globals": {
    "React": "readonly",
    "render": "readonly",
    "cleanup": "readonly",
    "act": "readonly",
    "fireEvent": "readonly",
    "runAsyncTimers": "readonly",
    "spyOn": "readonly",
    "setWindowLocationObject": "readonly"
  },
  "parser": "babel-eslint",
  "rules": {
    "object-curly-newline": 0,
    "arrow-parens": ["error", "always"],
    "linebreak-style": ["error", "unix"],
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "import/no-cycle": 0,
    "import/order": 0,
    "react/no-did-update-set-state": 0,
    "import/prefer-default-export": 0,
    "max-len": [
      "error",
      {
        "code": 120,
        "ignoreComments": true,
        "ignoreUrls": true,
        "ignoreStrings": true
      }
    ],
    "no-confusing-arrow": 0,
    "no-console": [
      1,
      {
        "allow": ["warn", "error"]
      }
    ],
    "comma-dangle": ["error", "always-multiline"],
    "quote-props": 0,
    "react/jsx-filename-extension": 0,
    "react/jsx-one-expression-per-line": 0,
    "react/prop-types": 0,
    "react/jsx-props-no-spreading": 0,
    "jsx-a11y/label-has-associated-control": 0,
    "spaced-comment": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "operator-linebreak" : 0
  }
}
