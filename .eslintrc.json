{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "globals": {
    "React": "readonly",
    "render": "readonly",
    "cleanup": "readonly",
    "act": "readonly",
    "fireEvent": "readonly",
    "runAsyncTimers": "readonly",
    "spyOn": "readonly",
    "setWindowLocationObject": "readonly"
  },
  "parser": "babel-eslint",
  "rules": {
    "react/no-unescaped-entities": "off",
    "@next/next/no-page-custom-font": "off"
  }
}
